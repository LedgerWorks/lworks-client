{
  "name": "lworks-client",
  "version": "2.7.0-alpha",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "url": "https://github.com/LedgerWorks/lworks-client"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf ./dist && npm test",
    "build:types": "tsc",
    "prebuild:ts": "npx typedoc",
    "build:ts": "esbuild src/index.ts --bundle --platform=node --target=node16 --outfile=dist/index.js --external:./node_modules/*",
    "build": "npm-run-all build:types",
    "start": "node dist/index.js",
    "lint": "eslint --ext .ts ./src && prettier --check ./src",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "node ./scripts/update-library-version.js",
    "test": "jest",
    "dev:run": "esbuild src/dev.ts --bundle --sourcemap --outdir=dist/dev --platform=node --target=node18 && node -r dotenv/config dist/dev/dev.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/aws4": "^1.11.2",
    "@types/jest": "^29.2.3",
    "@types/json-logic-js": "^2.0.1",
    "@types/node": "^18.7.7",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "dotenv": "^16.0.1",
    "esbuild": "^0.15.11",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "aws4": "^1.12.0",
    "jest": "^29.3.1",
    "json-logic-js": "^2.0.2",
    "mixpanel": "^0.17.0",
    "node-fetch": "^2.6.7",
    "pino": "^8.7.0",
    "tiny-invariant": "^1.3.1"
  },
  "keywords": [
    "LedgerWorks",
    "lworks",
    "Hedera",
    "hashgraph",
    "web3"
  ]
}
